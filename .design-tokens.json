{
  "fontFamily": "Fira Sans",
  "fontFamilyMono": "Fira Mono",
  "colors": {
    "background": "#282a36",
    "text": "#f8f8f2",
    "primary": "#a4ffff",
    "secondary": "#69ff94",
    "accent": "#e06c75",
    "backgroundLighter": "#343746",
    "backgroundLight": "#2E303E",
    "backgroundDark": "#22242E",
    "backgroundDarker": "#1C1D26",
    "textLighter": "#FFFFFF",
    "textLight": "#FFFFFF",
    "textDark": "#DEDEC2",
